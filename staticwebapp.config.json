
{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "mimeTypes": {
    ".json": "application/json"
  },
  "globalHeaders": {
    "cache-control": "must-revalidate, max-age=6000"
  },
  "auth": {
    "rolesSource": "/api/GetRoles",
    "identityProviders": {
      "customOpenIdConnectProviders": {}
    }
  }
}
